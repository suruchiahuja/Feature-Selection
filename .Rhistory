library("ElemStatLearn")
library("clustvarsel")
data(nci)
dim(nci)
dat <- t(nci)
dat <- scale(dat)
pc.fit <- prcomp(dat, .scale=TRUE)
pc.fit.x <- pc.fit$x[,1:2]
clust.fit <- clustvarsel(pc.fit.x, G = 1:9, search = "headlong")
clust.fit <- clustvarsel(pc.fit.x, G = 1:5, search = "headlong")
clust.fit <- clustvarsel(pc.fit.x, G = 1:9, search = "greedy")
clust.fit
clust.fit <- clustvarsel(pc.fit.x, G = 1:9, search = "greedy", direction = "forward")
clust.fit
clust.fit$subset
